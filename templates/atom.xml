<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>{{{BLOG_TITLE}}}</title>
  <subtitle>{{{BLOG_DESCRIPTION}}}</subtitle>
  <link href="{{{BLOG_URL}}}"/>
  <updated>{{{CURRENT_DATE}}}</updated>
  <author>
    <name>{{{AUTHOR}}}</name>
    <email>{{{EMAIL}}}</email>
  </author>

  {{#ITEMS}}
  <entry>
    <title>{{{TITLE}}}</title>
    <link href="{{{POST_LINK}}}"/>
    <id>{{{POST_LINK}}}</id>
    <updated>{{{DATE}}}</updated>
    <content type="html">
      {{CONTENT}}
    </content>
  </entry>
  {{/ITEMS}}

</feed>
